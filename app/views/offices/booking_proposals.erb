<h1> <%= @office.name %> </h1>

<% @office.booking_proposals.each do |temp_job| %>
  <hr/>

  <%=temp_job.when%>
  <%=temp_job.notes%>
<h3> Proposals </h3>
  <ul>
    <% temp_job.proposals.each do |proposal| %>
      <li>
        <%=proposal.professional.name%>
      </li>
    <% end %>
  </ul>
<%end%>

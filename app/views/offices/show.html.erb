<h1> <%= @office.name %> </h1>
<%= link_to "Temp Jobs", temp_jobs_office_path(@office) %>
|
<%= link_to "Booking Requests", booking_requests_office_path(@office) %>
|
<%= link_to "Booking Proposals", booking_proposals_office_path(@office) %>

<h2> Create new Booking Request </h2>
<%= form_for @office.booking_requests.new do |f| %>
  <%= f.label :notes%>
  <%= f.text_field :notes %>
  <br/>
  <%= f.label :when%>
  <%= f.date_field :when %>
  <%= f.hidden_field :office_id, value: @office.id %>
  <%= f.submit "Create" %>
<% end %>


<h2> Create new Temp Job Post </h2>
<%= form_for @office.temp_job_posts.new do |f| %>
  <%= f.label :notes%>
  <%= f.text_field :notes %>
  <br/>
  <%= f.label :when%>
  <%= f.date_field :when %>
  <%= f.hidden_field :office_id, value: @office.id %>
  <%= f.submit "Create" %>
<% end %>
